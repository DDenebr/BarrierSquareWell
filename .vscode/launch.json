{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            // Resolved by CMake Tools:
            "program": "${command:cmake.launchTargetPath}",
            "args": [
                    // "/gekko_home/raop0002/RandomOrganizing/dat/2024-4-24/phi_0.211_steady/DeltaE_0.215_random_critical_dump",  
                     "/home/raop0002/RandomOrganizing/dat/2024-4-25/DeltaE_0.111_fcc_critical_expand_extra2/phi_0.350_dump",
                     "output",
                     "1", "638", "0.35", "0.111", "17",
                     "Fst" //777
                     ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    // add the directory where our target was built to the PATHs
                    // it gets resolved by CMake Tools:
                    "name": "PATH",
                    "value": "$PATH:${command:cmake.launchTargetDirectory}"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Enable break on all exceptions",
                    "text": "catch throw",
                    "ignoreFailures": true
                },
                {
                    "description": "Ensures that a breakpoint for abort will be set!",
                    "text": "set breakpoint pending on",
                    "ignoreFailures": true
                },
                {
                    "description": "Breaks on failed asserts",
                    "text": "break abort",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}